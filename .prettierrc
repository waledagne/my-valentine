{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "spaceAfterFunctionName": "always",
  "overrides": [
    {
      "files": "*.php",
      "options": {
        "semi": true
      }
    },
    {
      "files": "*.vue",
      "options": {
        "semi": false
      }
    }
  ]
}
